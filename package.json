{
  "name": "@ifire/next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "NEXT_BUILD=1 next build",
    "beta:dev": "concurrently \"next dev --turbo\" \"tailwindcss --input styles/styles.css --output styles/globals.css --watch\"",
    "beta:build": "tailwindcss --input styles/styles.css --output styles/globals.css && NEXT_BUILD=1 next build && ./scripts/notFoundCleanup.js",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier -w './src/**/*.{tsx,ts}'",
    "test": "yarn lint && yarn tsc"
  },
  "dependencies": {
    "next": "^13.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^2.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@vercel/node": "^2.6.1",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.5.0",
    "eslint": "^8.30.0",
    "eslint-config-next": "^13.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.1.0",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "tailwindcss": "^3.2.2",
    "typescript": "^4.9.4"
  },
  "volta": {
    "node": "18.8.0"
  }
}
